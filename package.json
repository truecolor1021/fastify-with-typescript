{
  "name": "fastify-typescript-starter",
  "version": "3.0.0",
  "description": "Fastify TypeScript starter repository",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development nodemon",
    "debug": "nodemon --config nodemon-debug.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "rimraf dist && tsc",
    "prod": "cross-env NODE_ENV=production node dist/server.js"
  },
  "author": "Matschik <mathieu.schimmerling@protonmail.com>",
  "license": "MIT",
  "dependencies": {
    "fastify": "^2.13.1"
  },
  "devDependencies": {
    "@types/node": "^13.11.1",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  }
}
